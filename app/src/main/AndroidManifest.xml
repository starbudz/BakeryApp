<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bakeryApp1">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@drawable/icon"
        android:label="Sweets n Cakes"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.bakeryapp1">
        <activity android:name="com.example.bakeryApp1.admin.home.DisplayActivity"/>
        <activity android:name="com.example.bakeryApp1.admin.home.AdminHomeActivity"/>
        <activity android:name="com.example.bakeryApp1.admin.authenticate.ForgotPassword" />
        <activity android:name="com.example.bakeryApp1.admin.authenticate.RegisterAdmin" />
        <activity android:name="com.example.bakeryApp1.admin.authenticate.LoginAdmin" />
        <activity android:name="com.example.bakeryApp1.users.HomeActivity" />
        <activity android:name="com.example.bakeryApp1.welcome.OnboardingScreen" />
        <activity android:name="com.example.bakeryApp1.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>